model Account {
  id String @id @default(uuid()) @map("id")

  createdAt DateTime @default(now()) @map("created_at")
  updatedAt DateTime @default(now()) @updatedAt @map("updated_at")

  profiles      Profile[]
  authProviders AuthProvider[]
  blogPosts     BlogPost[]
  shards        Shard[]

  @@map("accounts")
}
